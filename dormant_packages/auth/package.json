{
	"name": "@ctx-core/auth",
	"version": "6.0.1",
	"description": "ctx-core auth",
	"main": "lib.js",
	"module": "lib.js",
	"types": "lib.ts",
	"scripts": {
		"build": "npm run copy-mjs && npm run compile",
		"clean": "rm -f tsconfig.tsbuildinfo && rm -f src/**/*.js && rm -f src/**/*.d.ts && rm -f src/**/*.map",
		"compile": "./node_modules/.bin/tsc -b .",
		"copy-mjs": "cd dist && echo \"module.exports = (async () => import('./index.js'))()\" > index.cjs",
		"prepublishOnly": "npm run clean && npm run build"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/ctx-core/ctx-core.git"
	},
	"keywords": [
		"ctx-core",
		"auth"
	],
	"author": "Brian Takita",
	"license": "Apache-2.0",
	"bugs": {
		"url": "https://github.com/ctx-core/ctx-core/issues"
	},
	"homepage": "https://github.com/ctx-core/ctx-core#readme",
	"publishConfig": {
		"access": "public",
		"cache": "~/.npm"
	},
	"devDependencies": {
		"typescript": "^3.7.2"
	},
	"gitHead": "43f719493f14189cc25cfaf4b8f16cf21bd3619f"
}
