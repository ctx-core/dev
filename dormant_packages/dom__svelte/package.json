{
	"name": "@ctx-core/dom__svelte",
	"version": "4.1.0",
	"description": "ctx-core dom__svelte",
	"main": "lib.js",
	"module": "lib.js",
	"types": "lib.ts",
	"scripts": {
		"build": "npm run copy-mjs && npm run compile",
		"clean": "rimraf tsconfig.tsbuildinfo && rm -f src/**/*.js && rm -f src/**/*.d.ts && rm -f src/**/*.map",
		"compile": "./node_modules/.bin/tsc -b .",
		"copy-mjs": "cd lib && echo \"module.exports = (async () => import('./index.js'))()\" > index.cjs",
		"prepublishOnly": "npm run clean && npm run build"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/ctx-core/ctx-core.git"
	},
	"keywords": [
		"ctx-core",
		"dom__svelte",
		"svelte"
	],
	"author": "Brian Takita",
	"license": "Apache-2.0",
	"bugs": {
		"url": "https://github.com/ctx-core/ctx-core/issues"
	},
	"homepage": "https://github.com/ctx-core/ctx-core#readme",
	"publishConfig": {
		"access": "public",
		"cache": "~/.npm"
	},
	"devDependencies": {
		"typescript": "^3.7.2"
	},
	"gitHead": "ce2109b69e46203ec12d693a8feb2859bcfc92c4"
}
