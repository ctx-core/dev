{
	"name": "@ctx-core/basic_auth",
	"version": "3.1.1",
	"description": "ctx-core basic_auth",
	"main": "env.js",
	"module": "env.js",
	"types": "env.ts",
	"scripts": {
		"build": "pnpm copy-mjs && pnpm compile",
		"clean": "rimraf tsconfig.tsbuildinfo && rm -f src/**/*.js && rm -f src/**/*.d.ts && rm -f src/**/*.map",
		"compile": "./node_modules/.bin/tsc -b .",
		"copy-mjs": "cd lib && echo \"module.exports = (async () => import('./index.js'))()\" > index.cjs",
		"prepublishOnly": "pnpm clean && pnpm build"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/ctx-core/ctx-core.git"
	},
	"keywords": [
		"ctx-core",
		"basic_auth"
	],
	"author": "Brian Takita",
	"license": "Apache-2.0",
	"bugs": {
		"url": "https://github.com/ctx-core/ctx-core/issues"
	},
	"homepage": "https://github.com/ctx-core/ctx-core#readme",
	"publishConfig": {
		"access": "public",
		"cache": "~/.npm"
	},
	"devDependencies": {
		"typescript": "^3.7.2"
	},
	"gitHead": "6448b6e05c211aa42d0113841f4d2dd0b11c1bae"
}
