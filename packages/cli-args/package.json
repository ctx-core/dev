	},
	"homepage": "https://github.com/ctx-core/ctx-core#readme",
	"publishConfig": {
		"access": "public"
	},
	"gitHead": "6448b6e05c211aa42d0113841f4d2dd0b11c1bae",
	"devDependencies": {
		"@ctx-core/array": "^15.3.8",
