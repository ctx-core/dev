{
	"name": "@ctx-core/store",
	"version": "11.1.10",
	"description": "ctx-core store",
	"main": "lib/index.js",
	"module": "lib/index.mjs",
	"types": "lib/index.d.ts",
	"files": [
		"lib/*",
		"store/*"
	],
	"scripts": {
		"build": "npm run clean && npm run compile",
		"clean": "rm -rf .rpt2_cache; find ./*/index.* -type f -not -name 'index.ts' -delete",
		"compile": "rollup -c",
		"prepublishOnly": "npm run compile",
		"test": "echo \"Error: no test specified\" && exit 1"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/ctx-core/ctx-core.git"
	},
	"keywords": [
		"ctx-core",
		"store"
	],
	"author": "Brian Takita",
	"license": "Apache-2.0",
	"bugs": {
		"url": "https://github.com/ctx-core/ctx-core/issues"
	},
	"homepage": "https://github.com/ctx-core/ctx-core#readme",
	"publishConfig": {
		"access": "public"
	},
	"dependencies": {
		"@ctx-core/array": "^14.0.9",
		"@ctx-core/combinators": "^4.0.7",
		"@ctx-core/dom": "^5.4.20",
		"@ctx-core/object": "^6.0.7",
		"svelte": "^3.6.2"
	},
	"devDependencies": {
		"rollup": "^1.16.3",
		"rollup-plugin-typescript2": "^0.21.2",
		"typescript": "^3.5.2"
	},
	"gitHead": "6448b6e05c211aa42d0113841f4d2dd0b11c1bae"
}
