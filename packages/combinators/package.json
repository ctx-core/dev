{
	"name": "@ctx-core/combinators",
	"version": "4.0.4",
	"description": "ctx-core combinators",
	"main": "lib/index.js",
	"module": "lib/index.mjs",
	"types": "lib/index.d.ts",
	"scripts": {
		"build": "npm run clean && npm run compile",
		"clean": "rm -rf .rpt2_cache; find ./*/index.* -type f -not -name 'index.ts' -delete",
